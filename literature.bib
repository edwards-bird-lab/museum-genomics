@article{yeatesMuseumsAreBiobanks2016,
	series = {Neuroscience * {Special} {Section} on {Insect} phylogenetics},
	title = {Museums are biobanks: unlocking the genetic potential of the three billion specimens in the world's biological collections},
	volume = {18},
	issn = {2214-5745},
	shorttitle = {Museums are biobanks},
	url = {http://www.sciencedirect.com/science/article/pii/S2214574516301596},
	doi = {10.1016/j.cois.2016.09.009},
	abstract = {Museums and herbaria represent vast repositories of biological material. Until recently, working with these collections has been difficult, due to the poor condition of historical DNA. However, recent advances in next-generation sequencing technology, and subsequent development of techniques for preparing and sequencing historical DNA, have recently made working with collection specimens an attractive option. Here we describe the unique technical challenges of working with collection specimens, and innovative molecular methods developed to tackle them. We also highlight possible applications of collection specimens, for taxonomy, ecology and evolution. The application of next-generation sequencing methods to museum and herbaria collections is still in its infancy. However, by giving researchers access to billions of specimens across time and space, it holds considerable promise for generating future discoveries across many fields.},
	language = {en},
	urldate = {2021-01-04},
	journal = {Current Opinion in Insect Science},
	author = {Yeates, David K and Zwick, Andreas and Mikheyev, Alexander S},
	month = dec,
	year = {2016},
	pages = {83--88},
	file = {ScienceDirect Snapshot:/Users/darencard/Zotero/storage/TYZGQ986/S2214574516301596.html:text/html;YeatesDY_2016_Current_Opinion_in_Insect_Science_Museums_are_biobanks_-_unlocking_the_genetic_potential_of_the_three_billion.pdf:/Users/darencard/Zotero/storage/5DTQRKUL/YeatesDY_2016_Current_Opinion_in_Insect_Science_Museums_are_biobanks_-_unlocking_the_genetic_potential_of_the_three_billion.pdf:application/pdf},
}

@article{mariniFiveMillionBird2020,
	title = {The five million bird eggs in the world’s museum collections are an invaluable and underused resource},
	volume = {137},
	issn = {0004-8038},
	url = {https://doi.org/10.1093/auk/ukaa036},
	doi = {10.1093/auk/ukaa036},
	abstract = {The {\textasciitilde}1.97 million egg sets ({\textasciitilde}5 million eggs) housed in museums have not been used in proportion to their availability. We highlight the wide variety of scientific disciplines that have used egg collections and the geographic locations and sizes of these collections, to increase awareness of the importance of egg collections, improve their visibility to the scientific community, and suggest that they offer a wealth of data covering large spatial scales and long time series for broad investigations into avian biology. We provide a brief history of egg collections and an updated list of museums/institutions with egg collections worldwide. We also review the limitations, challenges, and management of egg collections, and summarize recent literature based on historical and recent museum egg materials.The 5 million bird eggs in museum collections are an invaluable and underused resource that could be used for a variety of studies.We describe briefly the history of eggs that were collected worldwide over the last 200 years.We show that eggs from collections can be used to study ecology, behavior, evolution, classification, and species conservation.Several of the 300 institutions with egg collections that we list are already making them digitally available and physically accessible to scientists and the general public.We hope with this commentary to increase awareness of the importance of egg collections and improve their visibility and support.},
	number = {ukaa036},
	urldate = {2021-01-08},
	journal = {The Auk},
	author = {Marini, Miguel Ângelo and Hall, Linnea and Bates, John and Steinheimer, Frank D and McGowan, Robert and Silveira, Luis Fábio and Lijtmaer, Darío A and Tubaro, Pablo Luis and Córdoba-Córdoba, Sergio and Gamauf, Anita and Greeney, Harold F and Schweizer, Manuel and Kamminga, Pepijn and Cibois, Alice and Vallotton, Laurent and Russell, Douglas and Robinson, Scott K and Sweet, Paul R and Frahnert, Sylke and Corado, René and Heming, Neander Marcel},
	month = dec,
	year = {2020},
	file = {MariniMM_2020_The_Auk_The_five_million_bird_eggs_in_the_world’s_museum_collections_are_an_invaluable.pdf:/Users/darencard/Zotero/storage/Y9YXB3QL/MariniMM_2020_The_Auk_The_five_million_bird_eggs_in_the_world’s_museum_collections_are_an_invaluable.pdf:application/pdf;Snapshot:/Users/darencard/Zotero/storage/KQUPYEEJ/5875937.html:text/html},
}

@article{schindelNextGenerationNatural2018,
	title = {The next generation of natural history collections},
	volume = {16},
	issn = {1544-9173},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6062129/},
	doi = {10.1371/journal.pbio.2006125},
	abstract = {The last 50 years have witnessed rapid changes in the ways that natural history specimens are collected, preserved, analyzed, and documented. Those changes have produced unprecedented access to specimens, images, and data as well as impressive research results in organismal biology. The stage is now set for a new generation of collecting, preserving, analyzing, and integrating biological samples—a generation devoted to interdisciplinary research into complex biological interactions and processes. Next-generation collections may be essential for breakthrough research on the spread of infectious diseases, feeding Earth's growing population, adapting to climate change, and other grand research challenges. A decade-long investment in research collection infrastructure will be needed.},
	number = {7},
	urldate = {2021-01-08},
	journal = {PLoS Biology},
	author = {Schindel, David E. and Cook, Joseph A.},
	month = jul,
	year = {2018},
	pmid = {30011273},
	pmcid = {PMC6062129},
